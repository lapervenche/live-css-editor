{
  "name": "magic-css",
  "version": "8.18.6",
  "scripts": {
    "install-legacy-peer-deps": "npm install --legacy-peer-deps",
    "version": "./scripts/build-and-release/prepare-version/prepare-version.sh",
    "postversion": "git push --follow-tags",
    "lint": "eslint .",
    "manifest-generator": "node ./extension/manifest-generator.js",
    "copy-files-from-to": "copy-files-from-to",
    "copy-files-from-to-outdated": "copy-files-from-to --outdated",
    "copy-files-from-to-dev-mode": "copy-files-from-to --mode dev",
    "postinstall": "npm run copy-files-from-to",
    "get-latest-reviews": "node ./scripts/recipes/get-latest-reviews.js",
    "zip-chrome-extension": "node ./zip-extension.js chrome",
    "zip-edge-extension": "node ./zip-extension.js edge",
    "zip-firefox-extension": "node ./zip-extension.js firefox",
    "zip-opera-extension": "node ./zip-extension.js opera",
    "zip-extension": "npm run zip-chrome-extension && npm run zip-edge-extension && npm run zip-firefox-extension && npm run zip-opera-extension",
    "build-and-zip-extension": "npm run start:build-extension && npm run zip-extension",
    "all-is-well": "./scripts/all-is-well.sh",
    "test": "npm run all-is-well",
    "test:puppeteer": "cd tests ; npm test",
    "start:build": "cd extension/ ; ../node_modules/.bin/webpack --config ./webpack.config.js --env build.watch --env build.buildFor='publish'",
    "start:build-with-sourcemap": "cd extension/ ; ../node_modules/.bin/webpack --config ./webpack.config.js --env build.watch --env build.buildFor='development'",
    "start:build-inspect-brk": "cd extension/ ; node --inspect-brk ../node_modules/.bin/webpack --config ./webpack.config.js --env build.watch --env build.buildFor='development'",
    "start:build-extension": "cd extension/ ; node ../node_modules/.bin/webpack --config ./webpack.config.js --env build.buildFor='publish'",
    "start": "npm run start:build"
  },
  "dependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@react-hook/window-size": "^3.0.7",
    "babel-loader": "^8.2.5",
    "classnames": "^2.3.1",
    "codemirror": "=5.34.0",
    "crypto-js": "^4.1.1",
    "css-loader": "^5.2.4",
    "emmetio-codemirror-plugin-webextensions": "^0.3.7",
    "eslint-loader": "^4.0.2",
    "immer": "^9.0.12",
    "jquery": "^3.6.0",
    "mini-css-extract-plugin": "^1.5.0",
    "oauth-1.0a": "^2.2.6",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-command-palette": "=0.16.1",
    "react-dom": "^17.0.2",
    "react-joyride": "^2.4.0",
    "react-redux": "^7.2.4",
    "react-responsive": "^8.2.0",
    "react-roving-tabindex": "^3.2.0",
    "redux": "^4.2.0",
    "rooks": "^5.11.0",
    "socket.io": "^2.3.0",
    "toastr": "^2.1.4",
    "webpack-cli": "^4.9.2"
  },
  "devDependencies": {
    "@rbarilani/remove-source-map-url-webpack-plugin": "=1.1.0",
    "archiver": "^5.3.1",
    "chalk": "=4.1.2",
    "copy-files-from-to": "^3.6.0",
    "cp-file": "^9.1.0",
    "del": "^6.0.0",
    "eslint": "^7.24.0",
    "eslint-plugin-react": "^7.29.4",
    "git-hooks": "^1.1.10",
    "jsonfile": "^6.1.0",
    "note-down": "^0.2.3",
    "webpack": "^5.72.1"
  }
}
